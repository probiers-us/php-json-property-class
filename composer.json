{
  "name": "probiers-us/php-json-property",
  "description": "PHP JSON property helper trait",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Nick",
      "homepage": "https://github.com/nick-zh"
    }
  ],
  "keywords": [
    "php",
    "json",
    "property",
    "properties",
    "trait"
  ],
  "require": {
    "ext-json": "*",
    "php": "^7.4"
  },
  "require-dev": {
    "infection/infection": "^0.16",
    "phpunit/phpunit": "^9.1",
    "squizlabs/php_codesniffer": "^3.5",
    "phpstan/phpstan": "^0.12",
    "rregeer/phpunit-coverage-check": "^0.3.1",
    "johnkary/phpunit-speedtrap": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "ProbiersUs\\JsonProperty\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ProbiersUs\\JsonProperty\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}

